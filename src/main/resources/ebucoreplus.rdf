    <!-- http://www.ebu.ch/metadata/ontologies/ebucoreplus#Campaign -->
    <owl:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucoreplus#Campaign">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucoreplus#hasConsumptionCount"/>
