        <dcterms:description xml:lang="de">Um eine Eilmeldung zu beschreiben.</dcterms:description>
        <rdfs:label xml:lang="en">Breaking news item</rdfs:label>
        <rdfs:label xml:lang="fr">Derni&#xE8;res nouvelles</rdfs:label>
        <rdfs:label xml:lang="de">Eilmeldung</rdfs:label>
